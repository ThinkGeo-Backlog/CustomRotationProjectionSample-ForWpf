<Window x:Class="CustomRotationProjection.TestWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:my="clr-namespace:ThinkGeo.MapSuite.WpfDesktopEdition;assembly=WpfDesktopEdition"
    Title="Code Community-Custom Rotation Projection" Height="640" Width="800"
        Loaded="Window_Loaded" >
    <Grid Height="568">
        <my:WpfMap Name="wpfMap1" Width="778" Height="568" MouseMove="wpfMap1_MouseMove" />
        <TextBox Grid.Row="1" Height="31.108" Margin="0,0,0,-19.998" Name="textBox1" VerticalAlignment="Bottom" Grid.ColumnSpan="5" HorizontalAlignment="Left" Width="249.975" />
        <Button Height="28" Margin="340,0,0,-16" Name="btnRotateClockWise" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="162" Click="btnRotateClockWise_Click">Rotate ClockWise</Button>
        <Button Height="28" HorizontalAlignment="Right" Margin="0,0,96,-13.87" Name="btnRotateCounterClockWise" VerticalAlignment="Bottom" Width="162" Click="btnRotateCounterClockWise_Click">Rotate Counter ClockWise</Button>
    </Grid>
</Window>
